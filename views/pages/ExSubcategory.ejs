<%- include('header') %>

    <div class="container mt-4">
        <div class="row justify-content-center">

        </div>
    </div>

    <div class="container mt-4">
        <div class="row justify-content-center">
            <div class="col-lg-8 mb-4">
                <div class="table-responsive ">
                    <table class="table table-bordered table-striped">
                        <thead class="thead-dark">
                            <tr>
                                <th scope="col">No</th>
                                <th scope="col">Category Name</th>
                                <th scope="col">Category Image</th>
                                <th scope="col">Subcate Name</th>
                                <th scope="col">Date</th>
                                <th scope="col">Image</th>
                                <th scope="col">EXtrs cate name</th>
                                <th scope="col">Image</th>
                                <th scope="col">Action</th>
                                <!-- Add other table headers based on your data structure -->
                            </tr>
                        </thead>

                        <tbody>
                            <% exsubcat.map((val,i)=>{i++ %>
                                <tr>
                                    <td>
                                        <%= i %>
                                    </td>
                                    <td>
                                        <%= val.categoryId.cat_name %>
                                    </td>
                                    <td> 
                                        <img src="<%= val.categoryId.cat_image %> " alt="" width="100px" height="100opx">
                                    </td>
                                    <td> <%=  val.subcategoryId.subcat_name %></td>
                                    <td> <%=  val.subcategoryId.subcat_date %></td>
                                    <td> 
                                        <img src="<%= val.subcategoryId.subcat_image %> " alt="" width="100px" height="100opx">
                                    </td>
                                    <td> <%=  val.excat_name %></td>
                                    <td> 
                                        <img src="<%= val.Eximage %> " alt="" width="100px" height="100opx">
                                    </td>
                                    <td> 
                                        <a href="/exsubcatedelete?id=<%= val.id %>" class="btn btn-danger text-white">Delete</a>
                                        <a href="/exsubcateedit?id=<%= val.id %>" class="btn btn-success text-white">edit</a>
                                    </td>
                                </tr>
                                <% }) %>
                        </tbody>

                    </table>


                </div>
                <div class="d-flex justify-content-center">
                    <a href="/exsubcategoryAdd"><button class="btn btn-success text-white">Add New
                            ExSubCategory</button></a>
                </div>

                <div class="d-flex justify-content-center">
                    <a href="#" class="btn btn-warning  m-1">Car</a>
                    <a href="#" class="btn btn-danger text-white m-1">bike</a>
                    <a href="#" class="btn btn-secondary m-1">bus</a>
                    <a href="#" class="btn btn-secondary m-1">filter</a>
                </div>
            </div>

            <!-- Your Contact form HTML here -->

        </div>
    </div>
    <%- include('footer') %>